  <script>
    document.addEventListener('DOMContentLoaded', function() {
      const user = <%- JSON.stringify(user) %>;
      const loginSpinner = document.getElementById('login-spinner');
      if (loginSpinner) {
        loginSpinner.style.display = 'none';
      }

      if (user) {
        console.log(user);
        const bookForm = document.getElementById('guestBookForm');

        bookForm.elements['name'].value = user.NAME;
        bookForm.elements['email'].value = user.EMAIL;
        bookForm.elements['phone'].value = user.TEL_NO;
      } else {
        console.log('guest mode')
      }

      const navLinks = document.querySelectorAll('.nav-link');
      const sectionContents = document.querySelectorAll('.section-content');

      function showSection(sectionId) {
        sectionContents.forEach(content => {
          if (content.id === sectionId) {
            content.style.display = 'block';
          } else {
            content.style.display = 'none';
          }
        });
      }

      navLinks.forEach(link => {
        link.addEventListener('click', function(event) {
          event.preventDefault(); // Prevent default link behavior

          // Get the href attribute to determine which section to show
          const sectionId = this.getAttribute('href').substring(1); // Remove the #

          navLinks.forEach(navLink => {
            navLink.classList.remove('active');
          });

          link.classList.add('active');

          // Show the corresponding section
          showSection(sectionId);
        });
      });

      showSection('new_booking');

      function displayBookings(bookings) {
        const bookingListElement = document.getElementById('bookingSummaryList');
        if (bookingListElement) {
          // Create table wrapper
          const tableWrapper = document.createElement('div');
          tableWrapper.setAttribute('class', 'table-wrapper');

          // Create table and table header
          const table = document.createElement('table');
          table.setAttribute('class', 'booking-table'); // Optional: add a class for styling

          const thead = document.createElement('thead');
          const headerRow = document.createElement('tr');

          const headers = ['Tracking', 'Name', 'Capacity', 'Status'];
          headers.forEach(headerText => {
            const th = document.createElement('th');
            th.textContent = headerText;
            headerRow.appendChild(th);
          });

          thead.appendChild(headerRow);
          table.appendChild(thead);

          const tbody = document.createElement('tbody');

          bookings.forEach(booking => {
            const row = document.createElement('tr');

            const dateCell = document.createElement('td');
            dateCell.textContent = booking.ID_BOOKING;
            row.appendChild(dateCell);

            const nameCell = document.createElement('td');
            nameCell.textContent = booking.BOOKING_NAME;
            row.appendChild(nameCell);

            const capacityCell = document.createElement('td');
            capacityCell.textContent = booking.BOOKING_PAX;
            row.appendChild(capacityCell);

            const statusCell = document.createElement('td');
            statusCell.textContent = getStatusText(booking.BOOKING_STATUS);
            row.appendChild(statusCell);

            tbody.appendChild(row);
          });

          table.appendChild(tbody);
          tableWrapper.appendChild(table); // Append table to the wrapper
          bookingListElement.appendChild(tableWrapper); // Append wrapper to the booking list element
        }
      }

      async function fetchBookings() {
        try {
          let response;
          if (user) {
            if (user.ROLE === "ADMIN") {
              response = await fetch('/bookings'); // Adjust the URL as per your API endpoint
            } else {
              console.log(user.EMAIL)
              response = await fetch(`/bookings/fetch_by_email/${user.EMAIL}`);
            }

            if (!response.ok) {
              throw new Error('Network response was not ok');
            }

            const tables = await response.json();
            return tables;
          } else {
            response = await fetch('/bookings')
            const bookings = await response.json();
            return bookings;
          }
        } catch (error) {
          throw new Error('Error fetching tables: ' + error.message);
        }
      }

      if (user) {
        fetchBookings().then(bookings => {
          console.log(bookings);
          // displayTables(tables);
          displayBookings(bookings);
        }).catch(error => {
          console.error('Error fetching and displaying bookings:', error);
          //Handle error display if needed
        });
      }

      const guestBookingBtn = document.getElementById('guest-booking-btn');
      const bookingIdInput = document.getElementById('bookingId');
      // const bookingDetailsDiv = document.getElementById('bookingDetails');
      const bookingDetailsTable = document.getElementById('bookingDetailsTable');
      const bookingDetailsTableBody = bookingDetailsTable.querySelector('#bookingDetailsTableBody');
      const bookingNotFoundDiv = document.getElementById('bookingNotFound');

      guestBookingBtn.addEventListener('click', async function() {

        document.getElementById('guest-spinner').style.display = 'inline-block';

        if (!bookingIdInput.value) {
          document.getElementById('bookingInputWrn').style.display = 'block';
          document.getElementById('guest-spinner').style.display = 'none';
          return
        } else {
          document.getElementById('bookingInputWrn').style.display = 'none';
        }

        const bookingId = bookingIdInput.value.trim();

        try {
          // Fetch the list of bookings (already fetched and stored as `bookingsList`)
          const bookingsList = await fetchBookings(); // Assuming you have a function fetchBookings() that fetches bookings

          // Find the booking with the provided ID
          const foundBooking = bookingsList.find(booking => booking.ID_BOOKING === bookingId);

          if (foundBooking) {
            document.getElementById('guest-spinner').style.display = 'none';
            console.log('heho - ' + foundBooking)
            // Display the booking details
            //   bookingDetailsDiv.innerHTML = `
            // <p><strong>Booking Name:</strong> ${foundBooking.BOOKING_NAME}</p>
            // <p><strong>Date:</strong> ${new Date(foundBooking.BOOKING_DATE).toLocaleDateString()}</p>
            // <p><strong>Capacity:</strong> ${foundBooking.BOOKING_PAX}</p>
            // <p><strong>Status:</strong> ${getStatusText(foundBooking.BOOKING_STATUS)}</p>
            // `;
            bookingDetailsTableBody.innerHTML = `
          <tr>
            <td>${foundBooking.BOOKING_NAME}</td>
            <td>${new Date(foundBooking.BOOKING_DATE).toLocaleDateString()}</td>
            <td>${foundBooking.BOOKING_PAX}</td>
            <td>${getStatusText(foundBooking.BOOKING_STATUS)}</td>
          </tr>
        `;
            bookingDetailsTable.style.display = 'block';
            bookingNotFoundDiv.style.display = 'none';
          } else {
            // Display message for no booking found
            bookingDetailsTable.innerHTML = '';
            bookingNotFoundDiv.style.display = 'block';
            document.getElementById('guest-spinner').style.display = 'none';
          }
        } catch (error) {
          console.error('Error fetching or processing bookings:', error);
          // Handle error fetching bookings
        }
      });

      // Helper function to get status text based on status code
      function getStatusText(statusCode) {
        switch (statusCode) {
          case 'N':
            return 'New';
          case 'C':
            return 'Confirmed';
          case 'X':
            return 'Cancelled';
          default:
            return 'Unknown';
        }
      }

    });
  </script>

