                  <input type="password" id="password" name="password" class="form-control"  maxlength="50" required>
                </div>
                <div>
                  <button class="book-a-table-login" type="submit">Login</button>
                  <div id="login-spinner" class="spinner-grow spinner-grow-sm text-danger mx-3" role="status">
                    <span class="sr-only"></span>
                  </div>
                </div>
                <div>
                  <a href="/users/register_page"><i class="bi bi-pencil-square text-danger"></i>   Register new account here </a>
                </div>
                <div class="row mt-4">
                  <a href="/users/forgot_password"><i class="bi bi-question-octagon-fill text-danger"></i>   Forgotten Password? </a>
                </div>
              </form>
              <p id="message"></p>
            </div>
            <% } %>
          </div>
          <div class="col-lg-6 d-flex flex-column align-items-center reservation-form-bg" data-aos="fade-up" data-aos-delay="200">
