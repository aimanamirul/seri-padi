            <!-- Render content for logged-in user -->
            <div>
              <p class="mb-6">Hi, <%= user.NAME %>!</p>
              <h5 class="my-4">Summary of Bookings</h5>

              <div class="col-12" id="bookingSummaryList">

              </div>

              <% if (user.ROLE==="ADMIN" ) { %>
              <div class="my-4">
                <a href="/users/admin_page" class="fw-bold text-center">View Admin Dashboard</a>
              </div>
              <% } else { %>
              <a href="/users/bookings_page" class="my-2 text-center">Manage My Bookings</a>
              <% } %>

            </div>
            <% } else { %>

            <div class="login-container">
              <h4 class="my-4">Login</h4>
              <form id="loginForm">
                <div class="col-lg-6 form-group">
                  <label class="mb-2" for="username">Username:</label>
                  <input type="text" id="username" name="username" class="form-control"  maxlength="50" required>
